<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<types>
    <type name="SimpleHost" extends="TopologyObject">
        <property name="hostname" type="String" is-identity="true"/>
        <property name="name" type="String"/>
        <property name="os" type="String"/>
        <property name="cpuPercent" type="Metric" is-containment="true" unit-name="percent"/>
        <property name="memPercent" type="Metric" is-containment="true" unit-name="percent"/>
        <property name="netSentRate" type="Metric" is-containment="true" unit-name="byte/second"/>
        <property name="netRecvRate" type="Metric" is-containment="true" unit-name="byte/second"/>
        <property name="diskTotal" type="Metric" is-containment="true" unit-name="byte"/>
        <property name="diskFree" type="Metric" is-containment="true" unit-name="byte"/>
    </type>
</types>